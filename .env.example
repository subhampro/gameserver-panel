APP_ENV=production
APP_DEBUG=false
APP_KEY=base64:YOUR_APP_KEY_HERE
APP_THEME=pterodactyl
APP_TIMEZONE=Asia/Kolkata
APP_URL=https://panel.yourdomain.com
APP_LOCALE=en
APP_ENVIRONMENT_ONLY=false

# Log configuration
LOG_CHANNEL=daily
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Database configuration
DB_CONNECTION=mysql
DB_HOST=localhost
DB_PORT=3306
DB_DATABASE=panel
DB_USERNAME=pterodactyl
DB_PASSWORD=your_database_password

# Redis configuration
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Cache and queue drivers
CACHE_DRIVER=redis
QUEUE_CONNECTION=redis
SESSION_DRIVER=redis

# Hashids - used for obfuscating IDs in URLs
HASHIDS_SALT=random_salt_string_here
HASHIDS_LENGTH=8

# Mail configuration
MAIL_MAILER=smtp
MAIL_HOST=mail.yourdomain.com
MAIL_PORT=25
MAIL_USERNAME=panel
MAIL_PASSWORD=your_mail_password
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=no-reply@yourdomain.com
MAIL_FROM_NAME=YourPanel
# You should set this to your domain to prevent it defaulting to 'localhost', causing
# mail servers such as Gmail to reject your mail.
#
# @see: https://github.com/pterodactyl/panel/pull/3110
MAIL_EHLO_DOMAIN=panel.yourdomain.com

# Additional custom settings
APP_SERVICE_AUTHOR=your_email@yourdomain.com
PTERODACTYL_TELEMETRY_ENABLED=true
SESSION_SECURE_COOKIE=true

# Uncomment and set the following for advanced configurations
# RECAPTCHA_ENABLED=false
# RECAPTCHA_WEBSITE_KEY=
# RECAPTCHA_SECRET_KEY=
# PTERODACTYL_GUZZLE_TIMEOUT=15
# BACKUP_DRIVER=s3
# BACKUP_FILESYSTEM_DISK=s3
# BACKUP_FILESYSTEM_PATH=backups
